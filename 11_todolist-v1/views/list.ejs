<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= header %></h1>
    </div>
    <div class="box">
        <% for(let i=0; i<newItems.length; i++) { %>
            <div class="item">
                <input type="checkbox">
                <p><%= newItems[i] %></p>
            </div>
        <% } %>
    </div>
    <div class="box">
        <form method="POST" action="/">
            <input type="text" name="newItem" placeholder="Add New Item" autocomplete="off">
            <button type="submit" name="listType" value= <%= header %> >+</button>
        </form>
    </div>
<%- include("footer") -%>